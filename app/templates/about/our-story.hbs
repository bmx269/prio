<div class="overlay-container">
    <div class="bread-nav">
        <div class="container-fluid">
          <div class='row'>
          {{#link-to 'about.how-different' class='col text-right'}}How We're Different&nbsp;<i class="fas fa-caret-circle-down"></i>{{/link-to}}
      </div>
    </div>
  </div>
  {{#each page.section as |section|}}
    {{page-section section=section selectSnapshot=(action 'selectSnapshot')}}
  {{/each}}
  {{liquid-outlet enableGrowth=false}}
</div>
{{#with (filter-by "displayOn" "our-story" snapshots) as |snaps|}}
  {{snap-display snap=selectedSnapshot snaps=snaps}}
{{/with}}