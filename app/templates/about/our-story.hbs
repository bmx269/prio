<div class="overlay-container">
    <div class="bread-nav">
        <div class="container-fluid">
          <div class='row'>
          <LinkTo @route="about.your-priority" class="col text-right"><i class="fas fa-caret-circle-down"></i>&nbsp;What's Your Priority</LinkTo>
      </div>
    </div>
  </div>
  {{#each page.section as |section|}}
    <PageSection @section={{section}} @selectSnapshot={{action "selectSnapshot"}} />
  {{/each}}
  {{liquid-outlet enableGrowth=false}}
</div>
{{#with (filter-by "displayOn" "our-story" snapshots) as |snaps|}}
  <SnapDisplay @snap={{selectedSnapshot}} @snaps={{snaps}} />
{{/with}}