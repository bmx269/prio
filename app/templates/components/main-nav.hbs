      {{#nav.dropdown as |dd|}}
        {{#dd.toggle}}Menu <i aria-hidden="true" class="fa fa-bars"></i>{{/dd.toggle}}
        {{#dd.menu as |ddm|}}
          {{#ddm.item}}
            {{#ddm.link-to "about"}}About{{/ddm.link-to}}
            {{#navbar.nav as |nav|}}
              {{#nav.item}}
                {{#nav.link-to "about.our-story"}}Our Story{{/nav.link-to}}
              {{/nav.item}}
              {{#nav.item}}
                {{#nav.link-to "about.how-different"}}How We're Different{{/nav.link-to}}
              {{/nav.item}}
              {{#nav.item}}
                {{#nav.link-to "about.your-team"}}Meet Your Team{{/nav.link-to}}
              {{/nav.item}}
            {{/navbar.nav}}
          {{/ddm.item}}
          {{#ddm.item}}
            {{#ddm.link-to "financial-life"}}Financial Life Management{{/ddm.link-to}}
            {{#navbar.nav as |nav|}}
            {{#nav.item}}
                {{#nav.link-to "financial-life.management"}}Meet your one best financial life{{/nav.link-to}}
              {{/nav.item}}
              {{#nav.item}}
                {{#nav.link-to "financial-life.management.clarity"}}Find Clarity{{/nav.link-to}}
              {{/nav.item}}
              {{#nav.item}}
                {{#nav.link-to "financial-life.management.confidence"}}Gain Confidence{{/nav.link-to}}
              {{/nav.item}}
              {{#nav.item}}
                {{#nav.link-to "financial-life.management.control"}}Take  Control{{/nav.link-to}}
              {{/nav.item}}
            {{/navbar.nav}}
          {{/ddm.item}}
          {{#ddm.item}}
            {{#ddm.link-to "investment"}}Investment Management{{/ddm.link-to}}
          {{/ddm.item}}
          {{#ddm.item}}
            {{#ddm.link-to "news"}}What's New{{/ddm.link-to}}
            {{#navbar.nav as |nav|}}
              {{#nav.item}}
                {{#nav.link-to "news"}}Trending Stories{{/nav.link-to}}
              {{/nav.item}}
              {{#nav.item}}
                {{#nav.link-to "news"}}Archive{{/nav.link-to}}
              {{/nav.item}}
            {{/navbar.nav}}
          {{/ddm.item}}
          {{#ddm.item}}
            {{#ddm.link-to "contact"}}Contact{{/ddm.link-to}}
          {{/ddm.item}}
        {{/dd.menu}}
      {{/nav.dropdown}}
    {{/navbar.nav}}
  {{/navbar.content}}
{{/bs-navbar}}



